{
  "name": "logseq-dailyreflections-plugin",
  "version": "3.2.5",
  "effect": "false",
  "logseq": {
    "id": "logseq-dailyreflections-plugin",
    "title": "logseq-dailyreflections-plugin",
    "icon": "./icon.svg"
  },
  "main": "dist/index.html",
  "targets": {
    "main": false
  },
  "default": "dist/index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hkgnp/logseq-dailyreflections-plugin.git"
  },
  "author": "hkgnp",
  "license": "MIT",
  "description": "# Daily Reflections Plugin for Logseq (based on Creighton University's Daily Reflections)",
  "bugs": {
    "url": "https://github.com/hkgnp/logseq-dailyreflections-plugin/issues"
  },
  "homepage": "https://github.com/hkgnp/logseq-dailyreflections-plugin#readme",
  "scripts": {
    "build": "rm -R dist && echo 'dist folder removed'; parcel build --no-source-maps src/index.html --public-url ./"
  },
  "dependencies": {
    "@logseq/libs": "^0.0.10",
    "axios": "^1.1.3",
    "html-to-text": "^8.2.1",
    "logseq-dateutils": "^0.0.22"
  },
  "devDependencies": {
    "@types/html-to-text": "^8.1.1",
    "@types/node": "^18.11.9",
    "buffer": "^6.0.3",
    "parcel": "^2.7.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  }
}
